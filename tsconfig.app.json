/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts",
  ],
"include": [
    "src/**/*.d.ts"
  ],
  "exclude": [
    "server.ts",
    "**/*.server.*.ts",
    "**/*.server.ts",
    "test.ts",
    "**/*.spec.ts",
    "**/*.fake.ts",
    "**/*.test-helper.ts",
    "**/*.test-data.ts",
    "app/test/extensions/**/*.*"
  ]
}
